{
    "name": "authanram/generators",
    "type": "package",
    "description": "...",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/container": "^8.68.0",
        "illuminate/filesystem": "^8.68.0",
        "illuminate/pipeline": "^8.68.0",
        "illuminate/support": "^8.68.0",
        "webmozart/assert": "^1.10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.16.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/phpinsights": "^2.0.1",
        "pestphp/pest": "^1.20.0",
        "pestphp/pest-plugin-mock": "^1.0.3",
        "pestphp/pest-plugin-watch": "1.x-dev",
        "spatie/ray": "^1.30.3"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\Generators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\Generators\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "coverage": "@php ./vendor/bin/pest --coverage",
        "coverage:setup": "pecl install pcov",
        "fix": "export PHPINSIGHTS_ENV= && $PHP_INTERPRETER ./vendor/bin/phpinsights --fix",
        "insights": "export PHPINSIGHTS_ENV= && $PHP_INTERPRETER ./vendor/bin/phpinsights",
        "test": "@php ./vendor/bin/pest",
        "test:insights": "export PHPINSIGHTS_ENV=testing && $PHP_INTERPRETER ./vendor/bin/phpinsights analyse tests",
        "test:watch": "@php ./vendor/bin/pest --watch"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
